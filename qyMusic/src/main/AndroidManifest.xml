<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhuoren.musicplayer">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_reallaucher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name="com.zhuoren.musicplayer.MyService">
            <intent-filter>
                <action android:name="com.zhuoren.music.MyService"/>
            </intent-filter>
        </service>
                 
        <activity android:name="com.zhuoren.musicplayer.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
</application>



</manifest>